{
  "name": "OrangeScan",
  "version": "1.0",
  "manifest_version": 2,
  "description": "scan the domain",
  "permissions": [
    "tabs",
    "cookies",
    "http://*/*",
    "https://*/*",
    "background"
  ],
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "OrangeScan",
    "default_popup": "popup.html"
  }
}